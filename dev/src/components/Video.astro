---
import getVideoId from "get-video-id"
const { video } = Astro.props
const { id } = getVideoId(video.videoLink)
const url = `https://player.vimeo.com/video/${id}?dnt=1`
---
<iframe
  title="vimeo-player"
  src={url}
  width="640"
  height="360"
  frameborder="0"
  webkitallowfullscreen
  mozallowfullscreen
  allowfullscreen
>
</iframe>
